# Production Environment Variables - Copy to .env.production and fill in your values

# LLM Configuration
LLM_PROVIDER=openai
LLM_API_KEY=your_openai_api_key_here
LLM_MODEL=gpt-4

# Google Calendar API
GOOGLE_CLIENT_ID=your_google_client_id_here
GOOGLE_CLIENT_SECRET=your_google_client_secret_here
GOOGLE_REDIRECT_URI=https://your-backend-domain.railway.app/oauth/callback

# Database (SQLite for simplicity, can be upgraded)
DATABASE_URL=sqlite:///appointments.db

# Server Configuration
PORT=8000
ENVIRONMENT=production

# Security
JWT_SECRET_KEY=your_jwt_secret_key_here
CORS_ORIGINS=https://your-frontend-domain.vercel.app

# Logging
LOG_LEVEL=INFO